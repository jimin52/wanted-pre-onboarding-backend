{
  "name": "wanted-pre-onboarding-backend",
  "version": "1.0.0",
  "description": "원티드의 프리온보딩 선발과제를 제출하기 위한 레포지토리입니다.",
  "repository": "https://github.com:jimin52/wanted-pre-onboarding-backend.git",
  "main": "dist/src/index.js",
  "scripts": {
    "start": "npm run build && node dist/src/index.js",
    "build": "npx tsc -p .",
    "dev": "npm run db:init && nodemon --watch \"src/**/*.ts\" --exec \"ts-node\" src/index.ts",
    "prod:init": "npm run db:init && npm run start",
    "db:migrate": "npx prisma migrate dev --name init",
    "db:clean": "npx prisma db push --force-reset",
    "db:seed": "ts-node prisma/seed.ts",
    "db:init": "npm run db:clean && npm run db:seed"
  },
  "author": "jimin",
  "license": "ISC",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^6.1.0",
    "@prisma/client": "^5.4.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.18",
    "@types/node": "^20.8.3",
    "@types/swagger-ui-express": "^4.1.4",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
